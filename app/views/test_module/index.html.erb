<h1>検歴モジュール</h1>
<%= form_tag(url_for(action: :index), method: 'get') do %>
  <%= select_tag 'item_name', options_for_select([['T-Bil', 1],['AST', 2],['ALT', 3],['ALP', 4], ['gGTP', 5]]) %>
  <%= select_tag 'equation', options_for_select(['>','=','<']) %>
  <%= number_field_tag 'value' %>
  <%= submit_tag "search!" %>
<% end %>

<table class="table table-striped table-borderd table-hover">
  <thead>
    <tr>
      <th>登録ID</th><th>施設</th><th>登録日付</th><th>報告状況</th>
    </tr>
  </thead>
  <tbody id="items_list">
    <%= render partial: "items", object: @test_modules %>
  </tbody>
</table>
<%= paginate @test_modules %>

